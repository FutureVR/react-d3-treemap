    "D3",
    "Treemap",
    "motion",
    "animation",
    "zoomable",
    "zoom",
    "clickable"
  ],
  "author": {
    "name": "Jose Quinto Zamora",
    "email": "quintorel@hotmail.com",
    "url": "https://blog.josequinto.com"
  },
  "license": "BSD-3-Clause",
  "bugs": {
